.notiece-img {
  box-sizing: border-box;
  width: 100%;
  margin-top: 1.38vh;
}
.notiece-img  .swiper{
  height: 21vh;
  border-radius: 16rpx;
  overflow: hidden;
  box-shadow: 0rpx 0rpx 4rpx 2rpx  #44CAAC;
}
.notiece-img .swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  font-weight: bolder;
  color:#fff;
}
.img-item {
  width: 100%;
  height: 100%;
}