.count-circle-wrap {
  position: relative;
  width: 100%;
  height: 100%;
}
.count-circle-wrap .count-circle-container {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 20rpx solid #44caac;
  border-top-color: #303e53;
  transform: rotate(-45deg);
}
.count-main {
  box-sizing: border-box;
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 0 5%;
  border-radius: 50%;
  background-color: rgba(255,255,255,.5);
  transform: rotate(45deg);
}
.count-main .title {
  position: absolute;
  top: 15%;
  font-size: 15px;
  color: #9EA1A7;
}
.count-main .count-time {
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  width: 99%;
  font-family: Helvetica Neue,Consolas, Monaco, monospace;
  font-size: 40px;
  font-weight: 700;
  color:#233249;
  text-align: center;
  overflow: scroll;
  transform: translateY(-40%);
}