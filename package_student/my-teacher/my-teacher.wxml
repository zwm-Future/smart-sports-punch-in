<view class="my-teacher-wrap">
  <view class="header grey">
    {{currentSemester}}
  </view>
  <navigator url="./select-teacher/select-teacher" class="main grey">
    <view class="main-left">
      <view class="main-header">
        <view class="tip-title">课程信息</view>
        <view class="teacher-name">{{teacherName}}</view>
      </view>
      <view class="course-info">
        <view class="info-mes">授课老师及班级</view>
        <view class="sport-info">{{courseInfo}}</view>
      </view>
    </view>
    <view class="main-right">
      <mp-icon extClass="icon" icon="arrow" color="#1a1a1a"></mp-icon>
    </view>
  </navigator>
</view>