.type-wrap {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding: 0 30rpx;
  font-size: 16px;
  color:#fff;
  background: linear-gradient(to left,#44caac,#44c2ca);
  border-radius: 10px;
}
.type-wrap .type-title {
  display: flex;
  align-items: center;
  width: 100%;
  height: 54px;
}
.type-wrap .type-title .title-icon{
  width: 22px;
    height: 22px;
}
.type-wrap .type-title .title-text{
  font-weight: bolder;
}
.type-wrap .select-type-container {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20px;
}
.select-type-container .type-name {
  font-weight: bold;
}
.select-type-container .change-text {
  text-decoration: underline;
}
.type-wrap .punch-btn {
  width: 100%;
  height: 47px;
  /* line-height: 47px; */
}